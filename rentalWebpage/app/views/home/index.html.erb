<form action="search">
  <%= text_field :search_property, :property %>
  <%= button_to 'Search' %>
</form>
<table>
  <tr>
    <th>Address</th>
    <th>Apt</th>
    <th>City</th>
    <th>State</th>
    <th>Zipcode</th>
    <th>Bedrooms</th>
    <th>Bathrooms</th>
    <th>**Rating**</th>
  </tr>
  <% for property in @properties %>
    <tr>
      <td><%= property.address %></td>
      <td><%= property.apartment %></td>
      <td><%= property.city %></td>
      <td><%= property.state %></td>
      <td><%= property.zipcode %></td>
      <td><%= property.rooms %></td>
      <td><%= property.bathrooms %></td>
      <td>(1-5)</td>
    </tr>
  <% end %>
</table>
<%= link_to 'New Property', new_property_path %>
<%= link_to 'Edit Properties', properties_path %>
